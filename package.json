{
  "name": "trainquery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "rimraf dist/",
    "prebuild": "rimraf dist/",
    "dev": "run-p dev-server start-watch",
    "build": "run-p type-check build-only build-server",
    "build-server": "esbuild server/main.ts --bundle --platform=node --external:vite-ssr --minify --outfile=dist/server.js",
    "dev-server": "esbuild server/main.ts --bundle --platform=node --external:vite-ssr  --watch --outfile=dist/server.js",
    "start": "node dist/server.js",
    "start-watch": "nodemon --watch server/ dist/server.js",
    "test": "vitest",
    "build-only": "vite-ssr build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/ --write server/"
  },
  "dependencies": {
    "@vueuse/head": "^0.9.8",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "pinia": "^2.1.3",
    "vite-ssr": "^0.17.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "esbuild": "^0.18.13",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  }
}
