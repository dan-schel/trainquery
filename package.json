{
  "name": "trainquery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf -g dist/ data-*/ coverage/",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "tsx watch server/main.ts",
    "build": "vite-ssr build",
    "start": "cross-env NODE_ENV=production tsx server/main.ts",
    "test": "vitest --run",
    "test-coverage": "vitest --run --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "npm run lint-eslint && npm run lint-tsc && npm run type-check",
    "lint-eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --max-warnings 0",
    "lint-tsc": "tsc --project server/tsconfig.json --noemit && tsc --project shared/tsconfig.json --noemit",
    "format": "prettier --write src --write server --write shared --write tests",
    "tqlint": "tsx server/linter/main.ts"
  },
  "dependencies": {
    "@dan-schel/js-utils": "4.0.4",
    "@vueuse/head": "0.9.8",
    "adm-zip": "0.5.16",
    "chalk": "5.4.1",
    "csv-parser": "3.0.0",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "glob": "11.0.0",
    "long": "5.2.3",
    "luxon": "3.5.0",
    "mongodb": "6.12.0",
    "node-fetch": "3.3.2",
    "protobufjs": "7.4.0",
    "vite-ssr": "0.17.2",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "yaml": "2.6.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@tsconfig/node18": "18.2.4",
    "@types/adm-zip": "0.5.7",
    "@types/express": "4.17.21",
    "@types/jsdom": "21.1.7",
    "@types/luxon": "3.4.2",
    "@types/node": "18.19.68",
    "@vitejs/plugin-vue": "4.6.2",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vitest/coverage-v8": "2.1.8",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.1",
    "eslint-plugin-vue": "9.32.0",
    "jsdom": "25.0.1",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "sass": "1.83.0",
    "sass-loader": "16.0.4",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "0.20.5",
    "vitest": "^2.0.0",
    "vue-tsc": "2.1.10"
  },
  "engines": {
    "node": "20.19.2"
  }
}
