{
  "name": "trainquery",
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "tsx instance/index.ts",
    "build": "rimraf dist && vite build",
    "start": "cross-env NODE_ENV=production tsx instance/index.ts",
    "lint": "eslint . --max-warnings 0 && vue-tsc --noEmit -p tsconfig.json",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "setup": "tsx scripts/setup-instance.ts",
    "clean": "rimraf dist node_modules instance .env coverage data-*",
    "test": "vitest --run",
    "test-coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "@dan-schel/js-utils": "^3.2.2",
    "@universal-middleware/core": "^0.2.13",
    "@universal-middleware/express": "^0.2.9",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/server-renderer": "^3.5.13",
    "adm-zip": "^0.5.16",
    "chalk": "^5.3.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "glob": "^11.0.0",
    "long": "^5.2.3",
    "luxon": "^3.5.0",
    "mongodb": "^6.11.0",
    "node-fetch": "^3.3.2",
    "protobufjs": "^7.4.0",
    "unplugin-vue-markdown": "^0.26.2",
    "vike-vue": "^0.8.5",
    "vue": "^3.5.13",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/adm-zip": "^0.5.6",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@vitest/coverage-v8": "^2.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vike": "^0.4.204",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  },
  "files": [
    "dist",
    "index.ts",
    "server",
    "shared"
  ],
  "description": "This package is a work in progress - check back later for more information :)",
  "author": "dan-schel",
  "homepage": "https://github.com/dan-schel/trainquery",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dan-schel/trainquery.git"
  },
  "bugs": {
    "url": "https://github.com/dan-schel/trainquery/issues"
  }
}
